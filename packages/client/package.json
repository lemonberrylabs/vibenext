{
  "name": "@vibenext/client",
  "version": "0.1.0",
  "description": "React client components for VibeNext - an AI-powered coding assistant",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonberrylabs/vibenext.git",
    "directory": "packages/client"
  },
  "bugs": {
    "url": "https://github.com/lemonberrylabs/vibenext/issues"
  },
  "homepage": "https://github.com/lemonberrylabs/vibenext#readme",
  "keywords": ["vibenext", "ai", "claude", "nextjs", "react", "coding-assistant"],
  "files": ["dist", "README.md"],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./lib/controlPlane": {
      "types": "./dist/lib/controlPlane.d.ts",
      "import": "./dist/lib/controlPlane.js",
      "default": "./dist/lib/controlPlane.js"
    },
    "./server-actions": {
      "types": "./dist/server-actions.d.ts",
      "import": "./dist/server-actions.js",
      "default": "./dist/server-actions.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "next": "^15.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
